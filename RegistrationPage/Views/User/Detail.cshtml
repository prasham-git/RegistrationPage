@model RegistrationPage.Models.User

@{
    ViewBag.Title = "Detail";
}

<h2> User Details</h2>


    <div class="card" style="width: 18rem;">
        <div class="card-body">
            <h5 class="card-title">Name: @Model.Uname </h5>
        </div>
        <ul class="list-group list-group-flush">
            <li class="list-group-item"> Email: @Model.Uemail</li>
            <li class="list-group-item">Phone: @Model.Uphone</li>
            <li class="list-group-item">Gender: @Model.Ugender</li>
            <li class="list-group-item">Date of birth: @Model.Udob</li>
        </ul>
    </div>
     